<div class="link_to_steps">
  <div class="current_step">главный проект</div> 
  <div>Оценка главных аспектов BOCR</div>
  <div>Оценка индивидуальных аспектов</div>
  <div>Оценивание проектов</div>
  <div>Результат</div>
</div>
<%= link_to "Прекратить сравнение", compare_project_path, method: :delete %>

<%= form_for @compare_projects, url: compare_project_path(:compare_bocr), method: :get do |f| %>

<table class="all-projects">
  <tbody>
    <% @compare_projects.projects.each do |project| %>
      <tr>
        <td class="project-name-field">
            <%= project.name %>
        </td>
        <td class="accept-check">
          <%= f.radio_button "main_project_id", project.id %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= f.submit "Далее", class: "btn btn-primary compare-projs"%>
  <% end %>
