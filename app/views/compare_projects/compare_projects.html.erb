<div class="link_to_steps">
  <div>главный проект</div> 
  <div>Оценка главных аспектов BOCR</div>
  <div>Оценка индивидуальных аспектов</div>
  <div class="current_step">Оценивание проектов</div>
  <div>Результат</div>
</div>
<%= link_to "Прекратить сравнение", compare_project_path, method: :delete %>
compare_projects.aspects_priorities ---> <%= @compare_projects.aspects_priorities %>

<p>service <%= @aspects_priorities %> </p>


<%= form_for @compare_projects, url: compare_project_path(:get_global_ratings), method: :get, class: "compare-proc" do |f| %>

<table class="proj-range">
  <tbody>
    <% @compare_projects.aspects.each do |key, val| %>
      <% val.each do |asp| %>
      <tr class="<%= key.to_s %>">
        <td><%= asp.name%></td>
        <% @compare_projects.minor_prj.each do |project| %>
          <td>
            <%= @compare_projects.main_prj.name %>
            <%= f.range_field "[project_values][#{project.id}][#{val.name.downcase}][#{asp.name}]", value: "[]", in: -9..8, step: 1 %>
            <%= f.label "#{project.name}" %>
          </td>
        <% end %>
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<%= f.submit "Submit" %>
<% end %>
